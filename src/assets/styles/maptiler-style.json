{
  "version": 8,
  "name": "MapTiler Streets",
  "sources": {
    "maptiler_planet": {
      "type": "vector",
      "url": "https://api.maptiler.com/tiles/v3-openmaptiles/tiles.json?key=tKDOqJGURiimBRaaKrDJ"
    }
  },
  "glyphs": "https://api.maptiler.com/fonts/{fontstack}/{range}.pbf?key=tKDOqJGURiimBRaaKrDJ",
  "sprite": "https://api.maptiler.com/maps/streets/sprite?key=tKDOqJGURiimBRaaKrDJ",
  "layers": [
    {
      "id": "background",
      "type": "background",
      "paint": {
        "background-color": "#f8f4f0"
      }
    },
    {
      "id": "water",
      "type": "fill",
      "source": "maptiler_planet",
      "source-layer": "water",
      "paint": {
        "fill-color": "#a5bfdd"
      }
    },
    {
      "id": "landuse",
      "type": "fill",
      "source": "maptiler_planet",
      "source-layer": "landuse",
      "filter": ["in", "class", "grass", "park", "forest", "cemetery"],
      "paint": {
        "fill-color": "#d8e8c8"
      }
    },
    {
      "id": "roads",
      "type": "line",
      "source": "maptiler_planet",
      "source-layer": "transportation",
      "layout": {
        "line-join": "round",
        "line-cap": "round"
      },
      "paint": {
        "line-color": "#666",
        "line-width": 1
      }
    },
    {
      "id": "buildings",
      "type": "fill",
      "source": "maptiler_planet",
      "source-layer": "building",
      "paint": {
        "fill-color": "#d4d4d4",
        "fill-opacity": 0.7
      }
    }
  ]
}
